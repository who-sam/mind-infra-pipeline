{
  {
    "title": "MIND Notes Operations",
    "tags": ["mind", "notes", "operations"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "type": "row",
        "title": "Notes Operations Summary",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 0},
        "collapsed": false
      },
      {
        "id": 2,
        "type": "stat",
        "title": "Notes Created (Total)",
        "gridPos": {"h": 5, "w": 6, "x": 0, "y": 1},
        "targets": [
          {
            "expr": "mind_notes_created_total",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null}
              ]
            },
            "unit": "short"
          }
        },
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center",
          "textMode": "value_and_name"
        }
      },
      {
        "id": 3,
        "type": "stat",
        "title": "Notes Updated (Total)",
        "gridPos": {"h": 5, "w": 6, "x": 6, "y": 1},
        "targets": [
          {
            "expr": "mind_notes_updated_total",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "blue", "value": null}
              ]
            },
            "unit": "short"
          }
        },
        "options": {
          "colorMode": "background",
          "graphMode": "area"
        }
      },
      {
        "id": 4,
        "type": "stat",
        "title": "Notes Deleted (Total)",
        "gridPos": {"h": 5, "w": 6, "x": 12, "y": 1},
        "targets": [
          {
            "expr": "mind_notes_deleted_total",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": null}
              ]
            },
            "unit": "short"
          }
        },
        "options": {
          "colorMode": "background",
          "graphMode": "area"
        }
      },
      {
        "id": 5,
        "type": "stat",
        "title": "Notes Read (Total)",
        "gridPos": {"h": 5, "w": 6, "x": 18, "y": 1},
        "targets": [
          {
            "expr": "mind_notes_read_total",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "purple", "value": null}
              ]
            },
            "unit": "short"
          }
        },
        "options": {
          "colorMode": "background",
          "graphMode": "area"
        }
      },
      {
        "id": 6,
        "type": "timeseries",
        "title": "Notes Creation Rate",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 6},
        "targets": [
          {
            "expr": "rate(mind_notes_created_total[5m]) * 60",
            "legendFormat": "Notes/minute",
            "refId": "A"
          },
          {
            "expr": "rate(mind_notes_created_total[1h]) * 3600",
            "legendFormat": "Notes/hour",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 20,
              "pointSize": 5
            },
            "color": {"mode": "palette-classic"},
            "unit": "short"
          }
        }
      },
      {
        "id": 7,
        "type": "timeseries",
        "title": "Notes Modification Rate",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 6},
        "targets": [
          {
            "expr": "rate(mind_notes_updated_total[5m]) * 60",
            "legendFormat": "Updates/minute",
            "refId": "A"
          },
          {
            "expr": "rate(mind_notes_deleted_total[5m]) * 60",
            "legendFormat": "Deletes/minute",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 20,
              "pointSize": 5
            },
            "color": {"mode": "palette-classic"},
            "unit": "short"
          }
        }
      },
      {
