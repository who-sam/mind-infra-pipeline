      {
        "id": 15,
        "type": "row",
        "title": "Error Analysis",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 29},
        "collapsed": false
      },
      {
        "id": 16,
        "type": "timeseries",
        "title": "Notes Operation Errors",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 30},
        "targets": [
          {
            "expr": "rate(mind_notes_errors_total{operation=\"create\"}[5m])",
            "legendFormat": "Create Errors",
            "refId": "A"
          },
          {
            "expr": "rate(mind_notes_errors_total{operation=\"update\"}[5m])",
            "legendFormat": "Update Errors",
            "refId": "B"
